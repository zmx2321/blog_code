{
    "manifest_version": 3, //版本号，由google指定为2
    "name": "Reopen History", //插件名称
    "version": "1.1", //插件版本
    "description": "reopen recent closed history", //插件描述
    "icons": {
      //插件图标
      "128": "img/logo128.png",
      "48": "img/logo48.png",
      "16": "img/logo16.png"
    },
    "action": {
      "default_icon": "img/logo128.png", //插件图标
      "default_popup": "index.html" //点击图标后弹出的html互动文件
    },
    "permissions": ["history", "tabs"] //权限申请
  }
